            android:layout_marginTop="10dp"
            android:layout_height="wrap_content"
            android:layout_below="@id/pro_name2"
            android:textSize="12sp"
            android:textColor="#777"
            android:text="National, Florida\nG32 - 3453-321"/>
    </RelativeLayout>

